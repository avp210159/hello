[user]
	name = avp
	email = avpXXX...@ZZZ...
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  histt = log --pretty=format:\"%h T=%t %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[core]
	editor = vi
